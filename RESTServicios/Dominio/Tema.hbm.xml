<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="RESTServicios"
                   namespace="RESTServicios.Dominio"
                   default-lazy="false">
  <class name="Tema" table="tema">
    <id name="IdTema" column="idTema">
      <generator class="identity" />
    </id>
    <property name="DeTema" column="deTema" />
    <many-to-one name="Materia" column="materia" />
  </class>
</hibernate-mapping>
